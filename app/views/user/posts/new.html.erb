<header class="navbar navbar-expand-md navbar-info bd-info">
<div class="mx-auto" style="width: 300px;">
    <h1>
      今日のToDo
    </h1>
</div></br>
</header>
<%= form_with model: @post do |f| %>
    <div class="mx-auto" style="width: 300px;">
      <h2>タイトル</h2>
      <%= f.text_field :title %>
    </div></br>
    <div class="mx-auto" style="width: 300px;">
      <p>タスク</p>
      <p><%= f.text_field :list1 %></p>
      <p><%= f.text_field :list2 %></p>
      <p><%= f.text_field :list3 %></p>
      <p><%= f.text_field :list4 %></p>
      <p><%= f.text_field :list5 %></p>
      <p><%= f.text_field :list6 %></p>
      <p><%= f.text_field :list7 %></p>
      <p><%= f.text_field :list8 %></p>
      <p><%= f.text_field :list9 %></p>
      <p><%= f.text_field :list10 %></p>
    </div></br>
    <div class="mx-auto" style="width: 300px;">
    <%= f.submit '投稿', class: "btn btn-outline-primary btn-lg" %>
    </div>
<% end %>