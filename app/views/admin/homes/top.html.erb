<div class='container px-5 px-sm-0 mt-5'>
  <div class='row'>
    <!--左側のログアウト-->
    <div class='col-md-3'>
        <h2>管理者TOP</h2><br>
        <% if admin_signed_in? %>
          <%= link_to "ログアウト", destroy_admin_session_path, method: :delete, class: "btn btn-outline-danger btn-sm" %>
        <% end %>
    </div>
    
    <!--右側の会員一覧画面-->
    <div class='col-md-8 offset-md-1'>
      <h2>会員一覧</h2>
      <%= render 'admin/users/index', users: @users %>
    </div>
  </div>
</div>